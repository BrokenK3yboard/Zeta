{
  "schemaVersion": 1,
  "id": "zeta",
  "version": "${version}",
  "name": "Zeta",
  "description": "A fully featured Library designed around loading highly configurable, agile developed. modular mods",
  "authors": ["Vazkii", "quat", "IThundxr", "Siuolplex", "wiresegal", "MehVahdJukaar"],
  "contact": {
    "homepage": "https://violetmoon.org",
    "sources": "https://github.com/VazkiiMods/Zeta",
    "issues": "https://github.com/VazkiiMods/Zeta/issues"
  },
  "license": "https://github.com/VazkiiMods/Zeta/blob/master/LICENSE.md",
  "environment": "*",
  "entrypoints": {
    "main": [ "org.violetmoon.zeta.fabric.mod.ZetaFabricMod" ]
  },
  "mixins": [
    "zeta.mixins.json",
    "zeta-common.mixins.json"
  ],
  "depends": {
    "minecraft": "${minecraft_version}",
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": ">=${fabric_api_version}",

    "porting_lib_base": ">=${port_lib_base_version}"
  }
}
